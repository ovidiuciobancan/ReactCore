{
  "compilerOptions": {
    "baseUrl": "./",
    "module": "es2015",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "experimentalDecorators": true,
    "sourceMap": true,
    "skipDefaultLibCheck": true,
    "strict": true,
    "noImplicitAny": false,
    "lib": [ "es6", "dom" ],
    "types": [ "webpack-env", "material-ui" ],
    "paths": {
      "app/*": [ "./ClientApp/*" ],
      "config/*": [ "./ClientApp/code/config/*" ],
      "providers/*": [ "./ClientApp/code/providers/*" ],
      "store/*": [ "./ClientApp/store/*" ],
      "containers/*": [ "./ClientApp/components/container/*" ],
      "presentation/*": [ "./ClientApp/components/presentation/*" ]
    }
  },
  "exclude": [
    "bin",
    "node_modules"
  ]
}
